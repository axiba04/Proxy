{
  "experimental": {
    "clash_api": {
      "external_controller": "0.0.0.0:9090",
      "external_ui": "ui",
      "secret": "",
      "external_ui_download_url": "https://github.com/Zephyruso/zashboard/releases/latest/download/dist.zip",
      "external_ui_download_detour": "Default",
      "default_mode": "rule"
    },
    "cache_file": {
      "enabled": true,
      "store_fakeip": true
    }
  },
  "log": {
    "level": "info",
    "timestamp": true
  },
  "dns": {
    "servers": [
      {
        "tag": "proxy_dns",
        "type": "https",
        "server": "8.8.8.8",
        "detour": "Default"
      },
      {
        "tag": "cn_dns",
        "type": "udp",
        "server": "114.114.114.114"
      },
      {
        "tag": "fake_dns",
        "type": "fakeip",
        "inet4_range": "11.0.0.0/16",
        "inet6_range": "3ffe::/16"
      }
    ],
    "rules": [
      {
        "clash_mode": "direct",
        "server": "cn_dns"
      },
      {
        "clash_mode": "global",
        "server": "fake_dns"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "domain_suffix": ["ileidian.com", ".cn"]
          },
          {
            "domain_keyword": ["stun", "turn"]
          },
          {
            "rule_set": "cnsite"
          }
        ],
        "server": "cn_dns"
      },
      {
        "query_type": ["A", "AAAA"],
        "server": "fake_dns",
        "rewrite_ttl": 1
      }
    ]
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "tun-in",
      "interface_name": "singbox_tun",
      "address": ["172.18.0.1/30", "fdfe:dcba:9876::1/126"],
      "mtu": 9000,
      "auto_route": true,
      "route_exclude_address": ["10.0.0.0/8", "100.64.0.0/10", "169.254.0.0/16", "172.16.0.0/12", "192.0.0.0/24", "192.168.0.0/16"],
      "route_exclude_address_set": ["cnip"],
      "strict_route": true,
      "endpoint_independent_nat": false,
      "stack": "mixed"
    },
    {
      "type": "mixed",
      "tag": "mixed-in",
      "listen": "127.0.0.1",
      "listen_port": 7890
    }
  ],
  "outbounds": [
    {
      "type": "selector",
      "tag": "Default",
      "outbounds": ["Auto"]
    },
    {
      "type": "urltest",
      "tag": "Auto",
      "outbounds": [],
      "interval": "3m",
      "tolerance": 30
    },
    {
      "type": "selector",
      "tag": "Relay",
      "outbounds": ["Direct-Out"]
    },
    {
      "type": "selector",
      "tag": "AI-Service",
      "outbounds": ["Default", "Auto"]
    },
    {
      "type": "selector",
      "tag": "Telegram",
      "outbounds": ["Default", "Auto"]
    },
    {
      "type": "direct",
      "tag": "Direct-Out"
    }
  ],
  "route": {
    "final": "Default",
    "auto_detect_interface": true,
    "default_domain_resolver": "cn_dns",
    "rule_set": [
      {
        "tag": "cnsite",
        "type": "remote",
        "format": "binary",
        "url": "https://gh-proxy.org/https://github.com/MetaCubeX/meta-rules-dat/raw/refs/heads/sing/geo/geosite/cn.srs"
      },
      {
        "tag": "cnip",
        "type": "remote",
        "format": "binary",
        "url": "https://gh-proxy.org/https://github.com/MetaCubeX/meta-rules-dat/raw/refs/heads/sing/geo/geoip/cn.srs",
        "update_interval": "24h",
        "download_detour": "Direct-Out"
      },
      {
        "tag": "cngames",
        "type": "remote",
        "format": "binary",
        "url": "https://gh-proxy.org/https://github.com/MetaCubeX/meta-rules-dat/raw/refs/heads/sing/geo/geosite/category-games-!cn@cn.srs",
        "update_interval": "24h",
        "download_detour": "Direct-Out"
      },
      {
        "tag": "gfw",
        "type": "remote",
        "format": "binary",
        "url": "https://gh-proxy.org/https://github.com/MetaCubeX/meta-rules-dat/raw/refs/heads/sing/geo/geosite/gfw.srs",
        "update_interval": "24h",
        "download_detour": "Direct-Out"
      },
      {
        "tag": "aiservice",
        "type": "remote",
        "format": "binary",
        "url": "https://gh-proxy.org/https://github.com/MetaCubeX/meta-rules-dat/raw/refs/heads/sing/geo/geosite/category-ai-!cn.srs",
        "update_interval": "24h",
        "download_detour": "Direct-Out"
      },
      {
        "tag": "telegramip",
        "type": "remote",
        "format": "binary",
        "url": "https://gh-proxy.org/https://github.com/MetaCubeX/meta-rules-dat/raw/refs/heads/sing/geo/geoip/telegram.srs",
        "update_interval": "24h",
        "download_detour": "Direct-Out"
      },
      {
        "tag": "telegramsite",
        "type": "remote",
        "format": "binary",
        "url": "https://gh-proxy.org/https://github.com/MetaCubeX/meta-rules-dat/raw/refs/heads/sing/geo/geosite/telegram.srs",
        "update_interval": "24h",
        "download_detour": "Direct-Out"
      },
      {
        "tag": "onedrive",
        "type": "remote",
        "format": "binary",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/onedrive.srs",
        "update_interval": "24h",
        "download_detour": "Direct-Out"
      }
    ],
    "rules": [
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "port": 443
          },
          {
            "network": "udp"
          }
        ],
        "action": "reject"
      },
      {
        "network": "icmp",
        "outbound": "Direct-Out"
      },
      {
        "action": "sniff"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "protocol": "dns"
          },
          {
            "port": 53
          }
        ],
        "action": "hijack-dns"
      },
      {
        "ip_is_private": true,
        "outbound": "Direct-Out"
      },
      {
        "clash_mode": "direct",
        "outbound": "Direct-Out"
      },
      {
        "clash_mode": "global",
        "outbound": "Default"
      },
      {
        "protocol": "quic",
        "action": "reject"
      },
      {
        "protocol": "bittorrent",
        "outbound": "Direct-Out"
      },
      {
        "port": [3478, 5201],
        "outbound": "Direct-Out"
      },
      {
        "domain_suffix": ["ileidian.com", "ottiptv.cc"],
        "outbound": "Direct-Out"
      },
      {
        "rule_set": "telegramsite",
        "outbound": "Telegram"
      },
      {
        "rule_set": "aiservice",
        "outbound": "AI-Service"
      },
      {
        "rule_set": "cngames",
        "outbound": "Direct-Out"
      },
      {
        "rule_set": "gfw",
        "outbound": "Default"
      },
      {
        "action": "resolve"
      },
      {
        "rule_set": "telegramip",
        "outbound": "Telegram"
      },
      {
        "rule_set": "onedrive",
        "outbound": "Default"
      },
      {
        "rule_set": "cnip",
        "outbound": "Direct-Out"
      }
    ]
  }
}
